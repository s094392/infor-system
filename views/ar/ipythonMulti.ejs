<% include header.ejs %>
<div class="col-xs-2 col-md-2 ">
    <% include userDetials.ejs %>
</div>
<div class="col-xs-10 col-md-10 ">
    <ul class="breadcrumb">
      <li><a href="/">Home</a></li>
      <li><a href="/admin">Admin</a></li>
      <li class="active">Ipython</li>
    </ul>
    <form class="col-sm-offset-4 col-sm-4 form-horizontal" role="form" method="post">
	<fieldset>
	    <legend>New multiple ipython port</legend>
	    <div class="form-group">
		<label class="col-sm-3 control-label" for="number">Number</label>
		<div class="col-sm-9">
		    <input type="text" class="form-control" id="number" name="number" placeholder="Number" required>
		</div>
	    </div>

	    <div class="form-group">
		<label class="col-sm-3 control-label" for="last">Last Port</label>
		<div class="col-sm-9">
		    <input type="text" class="form-control" id="first" name="first" placeholder="Last Port" required>
		</div>
	    </div>

	    <div class="form-group">
		<div class="col-sm-offset-3 col-sm-9">
		    <button type="submit" class="btn btn-primary">Submit</button>
		</div>
	    </div>

	</fieldset>
    </form>
</div>
